//abstract class using override
abstract class Animal2{
    public  void Sound8(){
    }

}
class Dog2 extends Animal2{
    @Override
    public void Sound8(){
        System.out.println("dogs  barks");
    }
}
class Cat2 extends Animal2{
    @Override
    public  void Sound8(){
        System.out.println("Cats meows");
    }
}
class theory2{
    public static void main(String[]args){
        Dog2 obj = new Dog2();
        obj.Sound8();

        Cat2 obj2=new Cat2();
        obj2.Sound8();
    }
}
